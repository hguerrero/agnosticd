apiVersion: capabilities.3scale.net/v1alpha1
kind: Tenant
metadata:
  name: "{{ orgName }}-tenant"
  namespace: "{{ api_manager_namespace }}"
spec:
  email: "{{ tenantAdminEmail }}"
  masterCredentialsRef:
    name: system-seed
  organizationName: {{ orgName }}
  passwordCredentialsRef:
    name: api1-admin-secret
  systemMasterUrl: '{{ }}'
  tenantSecretRef:
    name: "{{ orgName}}-tenant-secret"
    namespace: "{{ gw_project_name }}"
  username: "{{ tenantAdminId }}"
